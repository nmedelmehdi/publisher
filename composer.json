{
	"name": "orchestra/publisher",
	"description": "Publisher Component for Orchestra Platform",
	"keywords": ["laravel", "orchestral", "orchestral-platform", "migration", "asset-publishing"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Publisher\\" : "src/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "5.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/console": "5.0.*",
		"illuminate/container": "5.0.*",
		"illuminate/database": "5.0.*",
		"illuminate/filesystem": "5.0.*",
		"orchestra/extension": "3.0.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/extension component outside of Orchestra Platform (~5.0).",
		"orchestra/foundation": "Allow using orchestra/extension with Orchestra Platform (~3.0)."
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.2-dev"
		}
	},
	"minimum-stability": "dev"
}
